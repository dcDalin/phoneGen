{
  "name": "phone-number-gen",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:coverage": "jest --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetTransformer.js"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "coveralls": "^3.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.0.5"
  }
}
